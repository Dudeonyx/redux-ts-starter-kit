    "prettier": "1.13.6",
    "tap-dot": "^2.0.0",
    "tape": "^4.9.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.0-dev.20180831"
  },
  "dependencies": {},
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  }
