{
  "baseUrl": "types",
  "typeRoots": ["types"],
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strict": true,
    "sourceMap": true,
    "baseUrl": ".",
    "target": "es5",
    "module": "commonJS",
    "outDir": "./dist",
    "moduleResolution": "node",
    "lib": ["es2015", "es2017", "dom"],
    "declaration": true,

  },
  "exclude": [
    "node_modules",
    "src/*.test.ts"
  ],
  "include": [
    "src",
    // "./examples/*.ts",
  ]
}
