{
    "compilerOptions": {
      "target": "es2015",
      "module": "commonjs",
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "strict": true,
      "composite": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "lib": ["es2016"],
      "esModuleInterop": true
    }
  }